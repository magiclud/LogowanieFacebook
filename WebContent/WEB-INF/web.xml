<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd"
	version="2.4">
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
	<context-param>
		<param-name>additionalPermissions</param-name>
		<param-value>email,user_birthday</param-value>
	</context-param>
	<context-param>
		<param-name>facebookAppId</param-name>
		<param-value>285808428255800</param-value>
	</context-param>
	<context-param>
		<param-name>facebookAppSecret</param-name>
		<param-value>3fb29eea26d94b747d8f595b3bc02225</param-value>
	</context-param>
	<servlet>
		<servlet-name>login</servlet-name>
		<servlet-class>com.fblogin.LoginServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>callback</servlet-name>
		<servlet-class>com.fblogin.CallbackServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>logout</servlet-name>
		<servlet-class>com.fblogin.LogoutServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>PrintCookieServlet</servlet-name>
		<servlet-class>com.fblogin.ServletDemoCookie</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>PrintCookieServletRemove</servlet-name>
		<servlet-class>com.fblogin.ServletDemoCookieDelete</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>login</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>callback</servlet-name>
		<url-pattern>/callback</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>logout</servlet-name>
		<url-pattern>/logout</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>PrintCookieServlet</servlet-name>
		<url-pattern>/printCookie</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>PrintCookieServletRemove</servlet-name>
		<url-pattern>/removeCookie</url-pattern>
	</servlet-mapping>
</web-app>